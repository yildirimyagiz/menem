{
  "name": "@acme/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    }
  },
  "license": "MIT",
  "prisma": {
    "schema": "./prisma/schema.prisma"
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "seed": "tsx prisma/seed.ts",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "db:generate": "pnpm run generate",
    "generate": "pnpm with-env prisma generate",
    "push": "pnpm with-env prisma db push --skip-generate",
    "studio": "pnpm with-env prisma studio --port 5556",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "@t3-oss/env-core": "^0.12.0",
    "stripe": "^18.1.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@acme/validators": "workspace:*",
    "@types/node": "^22.13.10",
    "@types/stripe": "^8.0.417",
    "dotenv-cli": "^8.0.0",
    "eslint": "catalog:",
    "faker": "^6.6.6",
    "faker-js": "^1.0.0",
    "prettier": "catalog:",
    "prisma": "^6.3.1",
    "tsx": "^4.19.3",
    "prisma-generator-typescript-interfaces": "^1.2.0",
    "typescript": "catalog:"
  },
  "prettier": "@acme/prettier-config"
}
