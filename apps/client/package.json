{
  "name": "@reservatior/nextjs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .cache .next .turbo node_modules",
    "dev": "pnpm with-env next dev -p 3000",
    "dev:mobile": "pnpm with-env next dev -p 3000",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "start": "pnpm with-env next start",
    "start:mobile": "pnpm with-env next start -p 3000",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "cap:init": "capacitor init",
    "cap:add:android": "capacitor add android",
    "cap:add:ios": "capacitor add ios",
    "cap:sync": "capacitor sync",
    "cap:build": "pnpm build && pnpm cap:sync",
    "cap:run:android": "pnpm cap:build && capacitor run android",
    "cap:run:ios": "pnpm cap:build && capacitor run ios",
    "cap:open:android": "capacitor open android",
    "cap:open:ios": "capacitor open ios",
    "cap:dev:android": "pnpm cap:sync && capacitor run android --live-reload --host localhost --port 3002",
    "cap:dev:ios": "pnpm cap:sync && capacitor run ios --live-reload --host localhost --port 3003",
    "analyze-locales": "tsx scripts/analyze-locales.ts",
    "organize-locales": "tsx scripts/organize-locales.ts",
    "locale-template": "tsx scripts/organize-locales.ts template",
    "check-translation-keys": "tsx scripts/check-translation-keys.ts",
    "translation-workflow": "tsx scripts/translation-workflow.ts",
    "translate-templates": "tsx scripts/translate-templates.ts"
  },
  "prettier": "@reservatior/prettier-config",
  "dependencies": {
    "@capacitor/android": "^7.4.2",
